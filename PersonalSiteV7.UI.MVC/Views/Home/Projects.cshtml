
@{
    ViewBag.Title = "Projects";
}

<h2>@ViewBag.Title</h2>

<div class="container">
    <div class ="row">
        <div class="col-lg-10 col-lg-offset-2">
            <div class="row centered mt mb">
                <div class="col-md-4">
                    <h4>Job Board</h4>
                    <p>
                        Job Board is my final project website at Centriq that I had created in 8 days under the supervision of a Centriq instructor, who was acting as the client and the project manager. My goal was to create an easy-to-use navigational experience while keeping a consistent look and feel throughout the whole site.
                    </p>
                    <img class="img-responsive" src="~/Content/img/JobsonsHomeScreen.jpg" href="http://fsdp.lexwoodward.com" />
                </div>
                @*<div class="col-lg-5 col-lg-offset-1 mt">
                    <img class="img-responsive" src="~/Content/img/JobsonsHomeScreen.jpg" href="http://fsdp.lexwoodward.com" />
                </div>*@
